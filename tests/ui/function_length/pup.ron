[
    // ModuleLint that enforces function length for the "test" module
    Module(
        ModuleLint {
            name: "long_function_test",
            matches: Module("^test$"),
            rules: [
                // Use RestrictFunctions rule to limit function length
                RestrictFunctions {
                    max_lines: 5,
                    severity: Warn
                }
            ]
        }
    ),
    
    // ModuleLint that enforces function length for "namespace_patterns::my_module"
    Module(
        ModuleLint {
            name: "namespace_patterns_test",
            matches: Module("^namespace_patterns::my_module$"),
            rules: [
                // Use RestrictFunctions rule with Error severity
                RestrictFunctions {
                    max_lines: 5,
                    severity: Error
                }
            ]
        }
    )
] 