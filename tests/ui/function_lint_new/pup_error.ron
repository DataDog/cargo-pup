(
    lints: [
        // Test function return type matcher with a small line limit to trigger warnings
        Function(
            (
                name: "function_returns_result_limit",
                matches: ReturnsType(Result),
                rules: [
                    MaxLength(
                        2,  // Maximum allowed function length (2 lines)
                        Warning, // Using Warning to match our stderr
                    )
                ]
            )
        )
    ]
) 